{
  "private": true,
  "name": "oice",
  "productName": "oice",
  "version": "0.0.0",
  "description": "omamori-voice",
  "author": {
    "name": "ke_tachibana",
    "email": "deadzone@be.to"
  },
  "electronVersion": "0.36.0",
  "watch": {
    "build:grammar": {
      "patterns": "grammar/charm.{yomi,grammar}",
      "extensions": "yomi,grammar"
    }
  },
  "scripts": {
    "test": "xo",
    "start": "electron .",
    "package": "electron-packager . $npm_package_productName --out=dist --ignore='^/dist$' --prune --asar --all --version=$npm_package_electronVersion",
    "build:grammar": "cd grammar; ./build.sh",
    "build": "npm run build:grammar & webpack",
    "watch": "npm-watch"
  },
  "main": "main/index.js",
  "files": [
    "main",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "electron-debug": "^0.5.0"
  },
  "devDependencies": {
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.36.0",
    "jquery": "^2.2.0",
    "npm-watch": "^0.1.1",
    "recorderjs": "git+https://github.com/mattdiamond/Recorderjs.git",
    "vue": "^1.0.16",
    "webpack": "^1.12.13",
    "xo": "^0.12.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
