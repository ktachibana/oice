{
  "private": true,
  "name": "oice",
  "productName": "oice",
  "version": "0.0.0",
  "description": "omamori-voice",
  "author": {
    "name": "ke_tachibana",
    "email": "deadzone@be.to"
  },
  "electronVersion": "0.36.0",
  "watch": {
    "build:grammar": {
      "patterns": "julius/grammar/charm.{yomi,grammar}",
      "extensions": "yomi,grammar"
    }
  },
  "scripts": {
    "test": "xo",
    "install-external": "node julius/platforms/install-external.js",
    "postinstall": "npm run install-external; npm run build",
    "start": "node main",
    "package": "electron-packager . $npm_package_productName --out=dist --ignore='^/dist$' --prune --platform=win32,darwin --arch=all --overwrite --version=$npm_package_electronVersion",
    "build:grammar": "cd julius/grammar; ./build.sh",
    "build": "npm run build:grammar; webpack",
    "watch": "npm-watch & webpack -w"
  },
  "main": "dist/index.js",
  "files": [
    "main",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.3",
    "express": "^4.15.4",
    "raw-loader": "^0.5.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "store": "^1.3.20",
    "superagent": "^1.8.3",
    "tmp": "0.0.28"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "npm-check-updates": "^2.6.3",
    "npm-watch": "^0.1.4",
    "recorderjs": "git+https://github.com/mattdiamond/Recorderjs.git",
    "webpack": "^1.13.0",
    "xo": "^0.14.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
